{
  "expo": {
    "name": "Hack Club",
    "slug": "hack-club-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hackclubapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "appleTeamId": "FBHQAXZYLW",
      "entitlements": {
        "com.apple.developer.security.application-groups": [
          "group.com.pizzalover125.mywidget",
          "group.com.pizzalover125.mywidget"
        ]
      },
      "bundleIdentifier": "com.pizzalover125.hackclubapp",
      "infoPlist": {
        "UIAppFonts": [
          "AntDesign.ttf",
          "Entypo.ttf",
          "EvilIcons.ttf",
          "Feather.ttf",
          "FontAwesome.ttf",
          "Foundation.ttf",
          "Ionicons.ttf",
          "MaterialIcons.ttf",
          "MaterialCommunityIcons.ttf",
          "SimpleLineIcons.ttf",
          "Octicons.ttf",
          "Zocial.ttf"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pizzalover125.hackclubapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}