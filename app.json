{
  "expo": {
    "name": "Hack Club",
    "slug": "hack-club-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hc-square.png",
    "scheme": "hackclubapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pizzalover125.hack-club-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hc-square.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pizzalover125.hackclubapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/hc-square.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/hc-h.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ec3750"
        }
      ],
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}